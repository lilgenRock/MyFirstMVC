@model WebAppFirstMVC.Models.Person

@{
    ViewBag.Title = "People";
}

<h2>People</h2>
<table class="table-bordered table-striped">
    <tr>
        <td>Name</td>
        <td>Age</td>
    </tr>
    @foreach (var item in WebAppFirstMVC.Models.Person.People)
    {
    <tr>
        <td>@item.Name</td>
        <td>@item.Age</td>
    </tr>
    }
</table>
